journal = {SOIL SCIENCE},
keywords = {CURIE-POINT PYROLYSIS,FLIGHT MASS-SPECTROMETRY,N-15 NMR-SPECTROSCOPY,NUCLEAR-MAGNETIC-RESONANCE,OFF-LINE THERMOCHEMOLYSIS,PYROLYSIS-GAS CHROMATOGRAPHY,SOIL ORGANIC-MATTER,SOLID-STATE C-13,TETRAMETHYLAMMONIUM HYDROXIDE TMAH,ULTRAVIOLET-LASER DESORPTION,pyrolysis gas chromatography-mass spectrometry,soft-ionization mass spectrometry,solid-state NMR,thermochemolysis},
