keywords = {CHEMICAL-COMPOSITION,DEGRADATION,DIGESTIBILITY,DOWN-REGULATION,FORAGE QUALITY VARIATION,INVITRO DEGRADABILITY,LIGNIN,MAIZE,RUMEN MICROORGANISMS,TRANSGENIC TOBACCO},
number = {2},
pages = {524--534},
publisher = {CROP SCIENCE SOC AMER},
title = {{Alfalfa stem tissues: Cell wall deposition, composition, and degradability}},
url = {http://apps.isiknowledge.com/full\_record.do?product=UA\&colname=WOS\&search\_mode=CitingArticles\&qid=32\&SID=W1Ad1MhL1J1nejkjoKD\&page=14\&doc=135},
